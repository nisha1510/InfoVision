package core_java_day3;

public class Calculator {
		public int add(int no1,int no2) {
			int result = no1 + no2;
			System.out.println(result);
			return result;
		}
		public float add(float no1,float no2) {
			float result = no1 + no2;
			System.out.println(result);
			return result;
		}
		public int add(int a) {
			int result = a+a;
			System.out.println(result);
			return result;
		}
	
}
